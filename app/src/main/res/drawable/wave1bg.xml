<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Background Rectangle (Solid Color) -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#EDEAE6"/> <!-- Set your desired background color here -->
        </shape>
    </item>

    <!-- Vector Shape -->
    <item android:drawable="@drawable/wave"/> <!-- Replace "your_vector_drawable_name" with the actual name of your vector drawable -->

</layer-list>